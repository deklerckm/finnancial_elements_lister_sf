{
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "bracketSpacing": true,
    "endOfLine": "auto",
    "plugins": [
        "./node_modules/prettier-plugin-packagejson",
        "./node_modules/prettier-plugin-jsdoc"
    ],
    "arrowParens": "always",
    "printWidth": 90,
    "jsdocSingleLineComment": false
}
